{
  "template_file": "template.php",
  "css_file": ["style.css"],
  "minify": false,
  "generate": [
    "template_cool.html",
    "template_awesome.html",
    "template_yeah.html"
  ],
  "placeholders": {
    "DEVELOPED_BY": "Developed by Your Company",
    "I_LIKE": "PIZZA",
    "PLACEHOLDER_EXAMPLE": "This text was specified in the compile.json file"
  },
  "output_dir": "../dist",
  "zip": {
    "filename": "compiled_templates.zip",
    "files": [
      "template_cool.html",
      "template_awesome.html",
      "template_yeah.html",
      "img/htmlmailcompiler-logo.png"
    ]
  }
}